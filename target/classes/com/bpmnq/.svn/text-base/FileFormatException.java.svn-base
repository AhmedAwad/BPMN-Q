package com.bpmnq;

/**
 * This exception type is thrown when the file format of an input file does not 
 * conform to the expected format (e.g. XML well-formedness, 
 * correct cardinalities in RDF, ...)  
 * @author Steffen Ryll
 */
public final class FileFormatException extends Exception {
    
    
    private static final long serialVersionUID = 8889491204934587635L;

    public FileFormatException() {
        super();
    }

    /**
     * @param message
     */
    public FileFormatException(String message) {
        super(message);
    }

    /**
     * @param message
     */
    public FileFormatException(Throwable message) {
        super(message);
    }

    /**
     * @param message
     * @param excp
     */
    public FileFormatException(String message, Throwable excp) {
        super(message, excp);
    }

}
